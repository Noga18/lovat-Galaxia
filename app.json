{
  "expo": {
    "name": "Galaxia Scout",
    "slug": "lovat-collection",
    "version": "26.0.1",
    "orientation": "default",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1f1f1f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.frc8033.lovatcollection",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#350868",
        "monochromeImage": "./assets/monochrome-icon.png"
      },
      "package": "com.frc8033.lovatcollection",
      "versionCode": 13
    },
    "androidNavigationBar": {
      "backgroundColor": "#1f1f1f"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "83168408-6a2d-4fa9-b6cc-98f3f9be7104"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "evaveksler",
    "plugins": [
      "expo-router",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "ALL"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "targetSdkVersion": 35
          }
        }
      ]
    ],
    "scheme": "lovat-collection"
  }
}
